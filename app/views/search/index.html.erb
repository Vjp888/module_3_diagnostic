<h1>Stations for <%= facade.zip_code %></h1>

<p>All Stations:</p>
<section class="all-stations">
  <% facade.get_stations.each do |station| %>
    <h3><%= station.name %></h3>
    <p><%= station.address %></p>
    <p><%= station.fuel_types %></p>
    <p><%= station.distance %></p>
    <p><%= station.access_times %></p>
  <% end %>
</section>
